                    <p><strong>Teachers:</strong></p>
                       <t t-foreach="student.teacher_ids" t-as="teacher">
                           <span><t t-esc="teacher.name"/> </span>
                       </t>
                </div>
            </t>
        </t>

    </template>
</odoo>
